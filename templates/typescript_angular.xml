<templateSet group="typescript_angular">
  <template name="serv" value="private $NAME$: $CLASS$" description="private service parameter" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="CLASS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="serb" value="public $NAME$: $CLASS$" description="public service parameter" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="CLASS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="nav" value="this.router.navigate([$ROUTE$]);" description="router navigation" toReformat="false" toShortenFQNames="true">
    <variable name="ROUTE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="navr" value="this.router.navigate([$ROUTE$], { relativeTo: this.route });" description="relative router navigation" toReformat="false" toShortenFQNames="true">
    <variable name="ROUTE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="rout" value="{&#10;    path: $PATH$,&#10;    component: $COMPONENT$&#10;}" description="component route definition" toReformat="true" toShortenFQNames="true">
    <variable name="PATH" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="COMPONENT" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="routr" value="{&#10;    path: '**',&#10;    redirectTo: '/$PATH$',&#10;    pathMatch: 'full'&#10;}" description="redirect route definition" toReformat="true" toShortenFQNames="true">
    <variable name="PATH" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="in" value="@Input() $NAME$: $TYPE$;" description="component input" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="TYPE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="TS_CLASS" value="true" />
    </context>
  </template>
  <template name="out" value="@Output() $NAME$: EventEmitter = new EventEmitter&lt;$TYPE$&gt;();" description="component output" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="TYPE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="TS_CLASS" value="true" />
    </context>
  </template>
  <template name="dial" value="let dialog: MatDialogRef&lt;$COMPONENT$&gt; = this.dialog.open($COMPONENT$);&#10;dialog.afterClosed()&#10;    .subscribe(($PARAMS$) =&gt; {&#10;        $END$&#10;    });" description="dialog creation" toReformat="false" toShortenFQNames="true">
    <variable name="COMPONENT" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="PARAMS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="TS_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>